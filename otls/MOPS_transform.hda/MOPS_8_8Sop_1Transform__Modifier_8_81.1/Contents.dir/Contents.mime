{
version 0.8
code	[ 0	locks=0	autoscope=0 ]	(	"// transform incoming points.
// rotation can be set explicitly or added to.

// user inputs.
vector T = translate;
vector R = rotate;
vector S = scale;
float U = uniform_scale;

// convert euler rotations to orient quaternion.
vector4 orient = eulertoquaternion(radians(R), 0);
// also store eulers in case of flipping rotations due to falloff later.
euler = R;

// set attributes.
scale = S;
uniform_scale = U;

vector4 mops_orient = quaternion(ident());
if(haspointattrib(0, \"mops_orient\")) {
    mops_orient = point(0, \"mops_orient\", @ptnum);
}

if(local) {
    // get existing xform, if any.
    vector p = P;
    //matrix3 m = primintrinsic(0, \"transform\", @ptnum);
    vector4 rot = quaternion(m);
    if(haspointattrib(0, \"mops_orient\")) {
        rot = point(0, \"mops_orient\", @ptnum);
    }
    //rot = qmultiply(mops_orient, rot);
    //rot = mops_orient;
    T = qrotate(rot, T);
}

P += T;"	)
