--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/MOPs_Parallel_Transport1/Parallel_transport.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 8	locks=0 ]	(	[ group	"" ] 	)
grouptype	[ 0	locks=0 ]	(	[ grouptype	0 ] 	)
class	[ 0	locks=0 ]	(	"primitive"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"string Nname = chs(\"Transported_Normal_Name\");
string Tname = chs(\"Transported_Tangent_Name\");
string BTname = chs(\"Transported_Bitangent_Name\");

int pnts[] = primpoints(0, @primnum);
int pntcnt = len(pnts);

vector firsttangent = normalize(point(geoself(), \"P\", pnts[1]) - point(geoself(), \"P\", pnts[0]));

vector firstnormal = point(0, \"N\", pnts[0]);
if(ch(\"custom_first_tangent\") > 0)
    firsttangent = normalize(chv(\"First_Tangent\"));
    
vector helper = normalize(cross(firstnormal, firsttangent));
firstnormal = normalize(cross(firsttangent, helper));


// forward declarations
vector binormal = {0,0,0};
float theta = 0;

vector tangents[] = {};
vector normals[] = {};


// fill arrays
for (int i = 0; i<pntcnt-1; i++)
    {
    push(tangents, normalize(point(geoself(), \"P\", pnts[i+1]) - point(geoself(), \"P\", pnts[i])));
    
    push(normals, firstnormal);
    }
push(tangents, tangents[pntcnt-2]);
push(normals, normals[pntcnt-2]);

// parallel transport
for (int i = 0; i<pntcnt-1; i++)
    {
    binormal = cross(tangents[i],tangents[i+1]);


    if (length(binormal) == 0)
        {
        normals[i+1] = normals[i];
        }
    else
        {
        binormal = normalize(binormal);
        theta = acos(dot(tangents[i], tangents[i+1]));
        matrix rotmat = ident(); 
        rotate(rotmat, theta, binormal); 
        normals[i+1] = rotmat * normals[i];
        }
    }
    

// set attributes
for (int j = 0; j < pntcnt; j++)
    {
    setpointattrib(geoself(), Tname, pnts[j], tangents[j], \"set\");
    setpointattrib(geoself(), Nname, pnts[j], normals[j], \"set\");
    binormal = normalize(cross(tangents[j], normals[j]));
    setpointattrib(geoself(), BTname, pnts[j], binormal, \"set\");

    
    }"	)
